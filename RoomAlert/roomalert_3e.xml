                    <snmp_oid>.1.3.6.1.4.1.20916.1.9.1.1.2.6.0</snmp_oid>
                    <key>dewpoint</key>
                    <history>90d</history>
                    <value_type>FLOAT</value_type>
                    <units>°C</units>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <parameters>
                                <parameter>0.01</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                </item>
